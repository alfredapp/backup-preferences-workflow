				<string>// Helpers
function envVar(varName) {
  return $.NSProcessInfo
    .processInfo
    .environment
    .objectForKey(varName).js
}

function dirContents(path) {
  return $.NSFileManager.defaultManager
    .contentsOfDirectoryAtURLIncludingPropertiesForKeysOptionsError(
      $.NSURL.alloc.initFileURLWithPath(path),
      undefined,
      $.NSDirectoryEnumerationSkipsHiddenFiles,
      undefined
    ).js?.map(p =&gt; p.path.js)
}

function run() {
  const backupPrefix = envVar("backup_prefix")
  const backupRegex = new RegExp(`${backupPrefix}.*\.tgz`)
  const backupsDir = envVar("backup_target")
  const backups = dirContents(backupsDir)?.filter(path =&gt; path.match(backupRegex))

  // "undefined" means the directory does not exist
  // "length zero" means it does not contain any matching files
  if (backups === undefined || backups.length === 0) return JSON.stringify({ items: [{
    title: "No Preference Backups Found",
    subtitle: `Run "${envVar("start_keyword")}" to set up backups`
  }]})

  const sfItems = backups
    .sort()
    .toReversed()
    .map(path =&gt; {
      const objPath = $(path)

      return {
        title: objPath
          .lastPathComponent
          .stringByDeletingPathExtension.js
          .replace(backupPrefix, ""),
        subtitle: objPath.stringByAbbreviatingWithTildeInPath.js,
        type: "file",
        arg: path
      }
    })

  return JSON.stringify({ items: sfItems })
}</string>
