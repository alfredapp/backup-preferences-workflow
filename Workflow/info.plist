				<string>// Helpers
function envVar(varName) {
  return $.NSProcessInfo
    .processInfo
    .environment
    .objectForKey(varName).js
}

// Detect misconfigurations early
function run() {
  // Syncing is on
  const defaultPrefs = "~/Library/Application Support/Alfred/Alfred.alfredpreferences"
  const currentPrefs = $(envVar("alfred_preferences")).stringByAbbreviatingWithTildeInPath.js
  if (defaultPrefs !== currentPrefs) return "SYNC_ON"

  // Time is invalid
  const time = envVar("backup_time").split(":").map(t =&gt; parseInt(t))
  if (time.length != 2 || time[0] &lt; 0 || time[0] &gt; 23 || time[1] &lt; 0 || time[1] &gt; 59) return "INVALID_TIME"

  // Count is invalid
  const count = envVar("backup_count")
  if (!/^\d+$/.test(count) || parseInt(count) &lt; 1) return "INVALID_COUNT"
}</string>
