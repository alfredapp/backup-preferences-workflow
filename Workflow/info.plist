readonly tmp_dir="$(mktemp -d)"
trap 'rm -rf "${tmp_dir}"' EXIT

# Extract backup to temporary directory
/usr/bin/tar \
  --directory "${tmp_dir}" \
  --extract --gzip --file "${restore_from}"

# Try to quit Alfred Preferences and abort if unable (e.g. editing an object)
if ! /usr/bin/osascript -l JavaScript -e 'Application("com.runningwithcrayons.Alfred-Preferences").quit()'
then
  echo -n 'PREFERENCES_OPEN' # For Alfred's conditional
  echo 'Could not quit Alfred Preferences' &gt;&amp;2 # For Alfred's debugger
  exit 1
fi

# Quit Alfred
/usr/bin/osascript -l JavaScript -e 'Application("com.runningwithcrayons.Alfred").quit()'

# Trash old preferences
/usr/bin/osascript -l JavaScript -e \
  'function run(argv) { Application("Finder").delete(Path(argv[0])) }' \
  "${alfred_preferences}"

# Move backup to correct location
/bin/mv "${tmp_dir}/Alfred.alfredpreferences" "${alfred_preferences}"

# Start Alfred
/usr/bin/osascript -l JavaScript -e 'Application("com.runningwithcrayons.Alfred").activate()'

# Show success notification
/usr/bin/osascript -l JavaScript -e \
  'function run(argv) { Application("com.runningwithcrayons.Alfred").runTrigger(argv[0], { inWorkflow: argv[1], withArgument: argv[2] }) }' \
  'restore_notification' \
  "${alfred_workflow_bundleid}" \
  "${restore_from}"</string>
